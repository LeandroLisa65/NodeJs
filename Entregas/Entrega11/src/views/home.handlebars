<h1 >Products</h1>
<a >Real Time Products</a>

{{#if products}}
<ul>
    {{#each products}}
    <li >
        <div>
            <p >{{this.title}}</p>
            <p >$ {{this.price}}</p>
            <p >{{this.description}}</p>
            <p >ID: {{this.id}}</p>
            <p >CODE: {{this.code}}</p>
            <p >STATUS: {{this.status}}</p>
            <p >STOCK: {{this.stock}}</p>
            <p >CATEGORY: {{this.category}}</p>
            <p >NÂº OF THUMBNAILS: {{this.thumbnails.length}}</p>
        </div>
         <div >
            {{#each this.thumbnails}}
                <img  src="{{this}}" alt="Product image">
            {{/each}}
        </div>
    </li>
    <hr >
    {{/each}}
</ul>
{{/if}}