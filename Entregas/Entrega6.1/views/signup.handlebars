<form action="/api/session/signup" method="post" class="container" style="width: 30%; margin-top:2%">
        <label for="first_name">
        First name
        <input type="text" name="first_name" id="first_name" placeholder="First Name">
        </label>
        <label for="last_name">
        Last Name <input type="text" name="last_name" id="last_name" placeholder="Last Name">
        </label>
        <label for="email">
        Email <input type="email" name="email" id="email" placeholder="Email">
        </label>
        <label for="password">
        Password <input type="password" name="password" id="password" placeholder="Password">
        </label>
        <input type="submit" value="Sign Up">
        
        <a role="button" href='/api/session/auth/github' style="width: 100%;margin-top:20px">Github Registration</a>
        <a role="button" href='/api/session/auth/google' style="width: 100%;margin-top:20px">Google Registration</a>
</form>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@picocss/pico@1/css/pico.min.css">